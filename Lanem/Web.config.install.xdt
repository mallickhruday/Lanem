<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="InsertIfMissing">
    <add key="Lanem_Log_Directory_Path" value="~\App_Data\error-logs" xdt:Transform="Insert" />
  </appSettings>
  <system.webServer>
    <modules>
      <add name="Lanem" type="Lanem.ErrorHandlerModule, Lanem" xdt:Transform="Insert" />
    </modules>
  </system.webServer>
</configuration>